  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "dev:local": "concurrently \"npm run dev:backend:debug\" \"npm run dev:frontend:local\"",
    "dev:backend:debug": "cross-env NODE_ENV=development tsx --inspect=0.0.0.0:9229 server/index.ts",
    "dev:frontend:local": "vite --config vite.config.local.ts",
    "dev:db": "node scripts/setup-local-db.js",
    "dev:setup": "npm run dev:db && echo 'âœ… Local development environment ready!'",
    "start": "NODE_ENV=production node dist/server/index.js",
    "build": "vite build --config vite.config.production.ts",
    "build:server": "tsc --project tsconfig.server.json || true",
    "build:all": "npm run build:server && npm run build",
    "build:local": "vite build --config vite.config.local.ts",
    "postbuild": "cp -r public dist/ && cp -r shared dist/",
    "check": "tsc --project tsconfig.migration.json --noEmit",
    "check:relaxed": "tsc --project tsconfig.migration.json --noEmit",
    "check:strict": "tsc --noEmit",
    "check:watch": "tsc --project tsconfig.migration.json --noEmit --watch",
    "check:errors": "tsc --project tsconfig.migration.json --noEmit 2>&1 | grep -c 'error TS' || echo '0 errors'",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:setup": "npm run db:generate && npm run db:push",
    "preview:local": "npm run build:local && serve -s dist/public -l 5173",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage"
  },
