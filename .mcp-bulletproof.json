{
  "mcpServers": {
    "serena": {
      "command": "uvx",
      "args": ["--from", "git+https://github.com/oraios/serena", "serena", "start-mcp-server"],
      "env": {}
    },
    "playwright": {
      "command": "cmd",
      "args": ["/c", "npx", "@playwright/mcp"],
      "env": {}
    },
    "in-memoria": {
      "command": "cmd", 
      "args": ["/c", "npx", "in-memoria", "server"],
      "env": {}
    }
  }
}